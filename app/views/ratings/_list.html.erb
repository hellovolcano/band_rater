<table id="ratings">
  <thead>
    <tr>
      <th>Band</th>
      <th>User</th>
      <th>Rating</th>
      <th>Last Updated</th>
    </tr>
  </thead>
  <% @ratings.each do |rating| %>
    <tr>
      <td><%= link_to rating.band.name, rating.band %></td>
      <td><%= link_to rating.user.name, rating.user %></td>
      <td><%= link_to rating.rating, rating %></td>
      <td><%= rating.updated_at %></td>
    </tr>
  <% end %>
</table>
